@model Lucid.Web.Portal.Models.RegisterModel

@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="margin-left:-400px; margin-top:-70px">

  <section class="loginform cf">

    Passwords must be at least @Membership.MinRequiredPasswordLength characters long.<br />


@using (Html.BeginForm()) {
    @Html.ValidationSummary()
    
    <ul  >
  

     <li> @Html.LabelFor(m => m.Email) 
            @Html.TextBoxFor(m => m.Email)  </li>

 <li> @Html.LabelFor(m => m.AccessCode)
             @Html.TextBoxFor(m => m.AccessCode)  </li>

 <li>
            @Html.LabelFor(m => m.Password)
            @Html.PasswordFor(m => m.Password)    </li>

            <li>
            @Html.LabelFor(m => m.ConfirmPassword)
                
        
  
            @Html.PasswordFor(m => m.ConfirmPassword)          
            </li>

    </ul>
    
                     
       
              
       
            
       
           
          
      
   
                  
              
       
              
       
            <input type="submit" value="Register" />}

      </section>
      </div>